<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
    
    <ui:with field='res' type='com.owb.playhelp.client.resources.Resources'/>
     
	<ui:style>
		.logo_small {
			background-color            :   #9152a0;
			width                       :   311px;
			height                      :   80px;
		}
		.logo_bar {
			width                       :   60px;
			height                      :   60px;
		}
		.mainBar {
			background-color            :   #9152a0;
		}
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel>
	   <g:Image width="300px" resource='{res.name2Logo}' />
	   <g:Image ui:field="newsPanel" styleName='{style.logo_bar}' resource='{res.newsLogo}' />
	   <g:Image ui:field="worldPanel" styleName='{style.logo_bar}' resource='{res.worldLogo}' />
	   <g:Image ui:field="friendPanel" styleName='{style.logo_bar}' resource='{res.friendLogo}' />
	   <g:Image ui:field="blogPanel" styleName='{style.logo_bar}' resource='{res.blogLogo}' />
	   <g:Image ui:field="radioPanel" styleName='{style.logo_bar}' resource='{res.radioLogo}' />
	   <g:Image ui:field="emailPanel" styleName='{style.logo_bar}' resource='{res.emailLogo}' />
	   <g:Label width="100px"></g:Label>
	   <g:Image ui:field="owbPanel" styleName='{style.logo_small}' resource='{res.owbLogo}' />
	</g:HorizontalPanel>
</ui:UiBinder>