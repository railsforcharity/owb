<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="../../resources/GlobalStyles.css">
		.important {
			font-weight: bold;
		}		
		.menuContainer {
			float: right;
			margin-top: 5px; 
			width: 300px;
		}
	</ui:style>
	<g:HTMLPanel styleName='{style.popup}'>
	    What need is required?
	    <g:ListBox ui:field="resourceList"/>	    
	    <g:VerticalPanel ui:field='contribPanel'/>
	  	<div class='{style.button}'>
			<g:Anchor href='javascript:;' ui:field='addLink'>add</g:Anchor>
			<g:Anchor href='javascript:;' ui:field='cancelLink'>cancel</g:Anchor>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 